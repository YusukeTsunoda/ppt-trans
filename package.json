{
  "name": "pptx-translator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3001": "next dev -p 3001",
    "dev:3002": "next dev -p 3002",
    "prebuild": "npm run type-check && node scripts/build-id-manager.js generate",
    "build": "next build",
    "postbuild": "node scripts/build-id-manager.js validate",
    "build:analyze": "ANALYZE=true npm run build",
    "build:canary": "npm run prebuild && DEPLOYMENT_TARGET=canary next build",
    "build:production": "npm run prebuild && DEPLOYMENT_TARGET=production next build",
    "start": "next start",
    "lint": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "validate": "npm run type-check && npm run lint && npm run test",
    "db:setup": "./scripts/setup-supabase.sh",
    "db:seed": "npx tsx scripts/setup-test-users.ts",
    "db:init": "npm run db:setup && npm run db:seed",
    "db:types": "npx supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.generated.ts",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@heroicons/react": "^2.2.0",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.55.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "file-type": "^21.0.0",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.26.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-auth": "4.24.11",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "xss": "^1.0.15",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/archiver": "^6.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
